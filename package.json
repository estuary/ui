{
    "name": "estuary-ui",
    "version": "1.0.0",
    "description": "A web based UI to assist in working with Estuary Flow",
    "main": "index.js",
    "private": true,
    "engines": {
        "node": ">=20",
        "npm": "^10"
    },
    "type": "module",
    "scripts": {
        "build": "vite build",
        "format": "prettier --check '**/*.{js,jsx,ts,tsx}'",
        "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "licenses": "licensee --errors-only",
        "lint": "eslint --ext js,jsx,ts,tsx .",
        "lint:fix": "eslint --fix --ext js,jsx,ts,tsx .",
        "maintenance:off": "rm public/maintenance",
        "maintenance:on": "touch public/maintenance",
        "pre_commit": "npm run licenses && npm run typecheck",
        "prepare": "husky install",
        "preview": "vite preview",
        "start": "npm run licenses && vite",
        "test": "vitest",
        "test:debug": "vitest --inspect-brk --no-file-parallelism",
        "test:ui": "vitest --ui",
        "typecheck": "tsc",
        "installDataPlane": "npm install 'https://gitpkg.now.sh/estuary/data-plane-gateway/client/dist?main'",
        "generate-flow-types": "cd deps/flow && ./fetch-flow.sh && ./flow-bin/flowctl json-schema | ./flow-bin/flowctl schemalate typescript --name Catalog --hoist-definitions > flow.d.ts",
        "generate-supabase-types": "node ./scripts/generateSupabaseTypes",
        "hack-in-local-web-flow": "./deps/flow-web/hack-in-local-copy.sh"
    },
    "dependencies": {
        "@date-fns/utc": "^1.0.0",
        "@dnd-kit/core": "^6.0.8",
        "@dnd-kit/modifiers": "^7.0.0",
        "@dnd-kit/sortable": "^8.0.0",
        "@dnd-kit/utilities": "^3.2.1",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@estuary/flow-web": "^0.6.0",
        "@jsonforms/core": "^3.2.1",
        "@jsonforms/material-renderers": "^3.2.1",
        "@jsonforms/react": "^3.2.1",
        "@microlink/react-json-view": "^1.23.0",
        "@monaco-editor/react": "^4.7.0",
        "@mui/icons-material": "^5.15.10",
        "@mui/lab": "^5.0.0-alpha.165",
        "@mui/material": "^5.15.10",
        "@mui/utils": "^5.15.11",
        "@mui/x-data-grid": "^6.11.2",
        "@mui/x-date-pickers": "^6.19.5",
        "@stripe/react-stripe-js": "^2.1.0",
        "@stripe/stripe-js": "^1.52.1",
        "@supabase-cache-helpers/postgrest-swr": "^2.0.1",
        "@supabase/postgrest-js": "^1.15.5",
        "@supabase/supabase-js": "^2.49.9",
        "@testing-library/jest-dom": "^6.4.2",
        "ansicolor": "^2.0.1",
        "data-plane-gateway": "https://gitpkg.now.sh/estuary/data-plane-gateway/client/dist?main",
        "date-fns": "^2.30.0",
        "echarts": "^5.4.3",
        "filter-obj": "^5.1.0",
        "highlight-words-core": "^1.2.3",
        "iconoir-react": "^7.7.0",
        "immer": "^9.0.21",
        "jose": "^4.15.5",
        "json-ptr": "^3.1.1",
        "json-refs": "^3.0.15",
        "lodash": "^4.17.21",
        "logrocket": "^10.1.0",
        "logrocket-react": "^5.0.1",
        "luxon": "^3.4.3",
        "material-ui-popup-state": "^5.0.10",
        "mnemonist": "^0.39.8",
        "monaco-editor": "^0.34.1",
        "notistack": "^3.0.1",
        "p-limit": "^5.0.0",
        "pretty-bytes": "^6.1.0",
        "react": "^17.0.2",
        "react-csv-downloader": "^3.1.0",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^4.0.10",
        "react-google-button": "^0.8.0",
        "react-helmet-async": "^2.0.5",
        "react-inspector": "^6.0.2",
        "react-intl": "^6.4.4",
        "react-mui-dropzone": "^4.0.6",
        "react-reflex": "^4.1.0",
        "react-router": "^6.18.0",
        "react-router-dom": "^6.18.0",
        "react-stay-scrolled": "^8.0.1",
        "react-use": "^17.4.0",
        "react-virtualized-auto-sizer": "^1.0.26",
        "react-window": "^1.8.9",
        "readable-numbers": "^1.0.7",
        "retry": "^0.13.1",
        "safe-stable-stringify": "^2.4.3",
        "stripe": "^12.1.1",
        "swr": "^2.2.5",
        "use-constant": "^1.1.1",
        "use-query-params": "^2.2.1",
        "use-resize-observer": "^9.1.0",
        "zustand": "^4.5.0"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
        "@testing-library/react": "^12.1.5",
        "@testing-library/user-event": "^14.5.2",
        "@types/highlight-words-core": "^1.2.3",
        "@types/lodash": "^4.14.191",
        "@types/logrocket-react": "^3.0.0",
        "@types/luxon": "^3.3.2",
        "@types/react": "^17.0.52",
        "@types/react-dom": "^17.0.18",
        "@types/react-inspector": "^4.0.6",
        "@types/react-window": "^1.8.5",
        "@types/retry": "^0.12.5",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/ui": "^3.1.2",
        "browserslist-to-esbuild": "^2.1.1",
        "eslint": "^8.57.1",
        "eslint-config-kentcdodds": "^20.3.1",
        "eslint-plugin-formatjs": "^4.11.3",
        "eslint-plugin-no-relative-import-paths": "^1.6.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^9.0.11",
        "jsdom": "^24.0.0",
        "licensee": "^11.1.1",
        "msw": "^2.0.11",
        "prettier": "^3.5.3",
        "rollup-plugin-license": "^3.6.0",
        "sharp": "^0.34.1",
        "typescript": "^4.9.4",
        "vite": "^6.3.6",
        "vite-plugin-checker": "^0.9.1",
        "vite-plugin-circular-dependency": "^0.5.0",
        "vite-plugin-compression2": "^1.3.3",
        "vite-plugin-image-optimizer": "^1.1.8",
        "vite-plugin-node-polyfills": "^0.23.0",
        "vite-plugin-sri3": "^1.0.6",
        "vite-plugin-wasm": "^3.4.1",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.1.2",
        "vitest-mock-extended": "^3.0.1"
    },
    "overrides": {
        "msw": {
            "path-to-regexp": "^8.0.0"
        }
    },
    "browserslist": {
        "production": [
            ">1%",
            "not dead",
            "Firefox ESR",
            "not ie < 11"
        ],
        "development": [
            "last 2 chrome version",
            "last 2 firefox version",
            "last 2 edge versions",
            "last 2 safari version"
        ]
    },
    "eslintIgnore": [
        "node_modules",
        "build",
        "*.json",
        ".vscode",
        "deps"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/estuary/ui.git"
    }
}
