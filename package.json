{
    "name": "estuary-ui",
    "version": "0.1.0-alpha",
    "description": "A web based UI to assist in working with Estuary Flow",
    "main": "index.js",
    "private": true,
    "scripts": {
        "start": "set WDS_SOCKET_PORT=0 && react-app-rewired start",
        "build": "react-app-rewired build",
        "test": "react-app-rewired test",
        "prepare": "husky install",
        "format": "prettier --check '**/*.{js,jsx,ts,tsx}'",
        "lint": "eslint --ext js,jsx,ts,tsx .",
        "lint:fix": "eslint --fix --ext js,jsx,ts,tsx .",
        "lint-staged": "lint-staged --config .lint-staged.js",
        "typecheck": "tsc",
        "generate-flow-types": "cd flow_deps && ./fetch-flow.sh && ./flow-bin/flowctl json-schema | ./flow-bin/flowctl schemalate typescript --name Catalog --hoist-definitions > flow.d.ts",
        "save-local-supabase-data": "cd supabase_data && ./fetch-connector_tags.sh",
        "__build_deploy__": "npm run build; serve -s build"
    },
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@jsonforms/core": "v3.0.0-beta.0",
        "@jsonforms/material-renderers": "v3.0.0-beta.1",
        "@jsonforms/react": "v3.0.0-beta.1",
        "@monaco-editor/react": "^4.4.4",
        "@mui/icons-material": "^5.6.1",
        "@mui/material": "^5.6.4",
        "@mui/utils": "^5.6.1",
        "@mui/x-data-grid": "^5.10.0",
        "@supabase/postgrest-js": "^0.37.2",
        "@supabase/supabase-js": "^1.35.2",
        "@supabase/ui": "^0.36.5",
        "ansicolor": "^1.1.100",
        "date-fns": "^2.28.0",
        "immer": "^9.0.12",
        "lodash": "^4.17.21",
        "monaco-editor": "^0.33.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.4",
        "react-intl": "^5.25.1",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0",
        "react-use": "^17.3.2",
        "serve": "^13.0.2",
        "swr": "^1.3.0",
        "use-constant": "^1.1.0",
        "zustand": "^4.0.0-rc.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^12.1.5",
        "@testing-library/user-event": "^14.1.1",
        "@types/auth0-js": "^9.14.6",
        "@types/lodash": "^4.14.182",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@types/react-lazylog": "^4.5.1",
        "eslint-config-kentcdodds": "^20.0.1",
        "eslint-plugin-jest-dom": "^4.0.1",
        "eslint-plugin-react": "^7.28.0",
        "husky": "^7.0.0",
        "jest-localstorage-mock": "^2.4.21",
        "jest-mock-extended": "^2.0.5",
        "lint-staged": "^12.4.1",
        "node-polyfill-webpack-plugin": "^1.1.4",
        "prettier": "^2.6.2",
        "react-app-rewired": "^2.2.1",
        "react-scripts": "5.0.0",
        "tsc-files": "^1.1.3",
        "typescript": "^4.6.3"
    },
    "browserslist": {
        "production": [
            ">1%",
            "not dead",
            "Firefox ESR",
            "not ie < 11"
        ],
        "development": [
            "last 2 chrome version",
            "last 2 firefox version",
            "last 2 edge versions",
            "last 2 safari version"
        ]
    },
    "eslintIgnore": [
        "node_modules",
        "build",
        "*.json",
        ".vscode",
        "flow_deps"
    ],
    "jest": {
        "resetMocks": false,
        "verbose": true,
        "testTimeout": 10000
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/estuary/ui.git"
    }
}
