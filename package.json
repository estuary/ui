{
    "name": "estuary-ui",
    "version": "0.1.0-alpha",
    "description": "A web based UI to assist in working with Estuary Flow",
    "main": "index.js",
    "private": true,
    "scripts": {
        "server": "cd temp-test-server && npm start",
        "start-all": "npm run server & npm start",
        "start": "set WDS_SOCKET_PORT=0 && react-app-rewired start",
        "build": "react-app-rewired build",
        "test": "react-app-rewired test",
        "prepare": "husky install",
        "format": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "lint": "eslint --ext js,jsx,ts,tsx .",
        "lint:fix": "eslint --fix --ext js,jsx,ts,tsx .",
        "lint-staged": "lint-staged --config .lint-staged.js",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@jsonforms/core": "v3.0.0-alpha.3",
        "@jsonforms/material-renderers": "v3.0.0-alpha.3",
        "@jsonforms/react": "v3.0.0-alpha.3",
        "@monaco-editor/react": "^4.3.1",
        "@mui/icons-material": "^5.3.1",
        "@mui/material": "^5.4.0",
        "@mui/utils": "^5.4.2",
        "@mui/x-data-grid": "^5.5.1",
        "auth0-js": "^9.19.0",
        "axios": "^0.25.0",
        "date-fns": "^2.28.0",
        "immer": "^9.0.12",
        "json-refs": "^3.0.15",
        "lodash": "^4.17.21",
        "monaco-editor": "^0.31.1",
        "prop-types": "^15.8.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.4",
        "react-intl": "^5.24.3",
        "react-router": "^6.2.1",
        "react-router-dom": "6.2.1",
        "react-use": "^17.3.2",
        "use-immer": "^0.6.0",
        "zustand": "^3.7.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@types/auth0-js": "^9.14.5",
        "@types/lodash": "^4.14.178",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "eslint-config-kentcdodds": "^20.0.1",
        "eslint-plugin-jest-dom": "^4.0.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "husky": "^7.0.0",
        "jest-localstorage-mock": "^2.4.19",
        "lint-staged": "^11.2.6",
        "node-polyfill-webpack-plugin": "^1.1.4",
        "prettier": "^2.4.1",
        "react-app-rewired": "^2.2.1",
        "react-scripts": "5.0.0",
        "tsc-files": "^1.1.3",
        "typescript": "^4.5.5"
    },
    "browserslist": {
        "production": [
            ">1%",
            "not dead",
            "Firefox ESR",
            "not ie < 11"
        ],
        "development": [
            "last 2 chrome version",
            "last 2 firefox version",
            "last 2 edge versions",
            "last 2 safari version"
        ]
    },
    "eslintIgnore": [
        "node_modules",
        "build",
        "*.json",
        ".vscode"
    ],
    "jest": {
        "resetMocks": false
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/estuary/ui.git"
    }
}
