{
    "name": "estuary-ui",
    "version": "0.2.0-alpha",
    "description": "A web based UI to assist in working with Estuary Flow",
    "main": "index.js",
    "private": true,
    "scripts": {
        "start": "set WDS_SOCKET_PORT=0 && react-app-rewired start",
        "build": "react-app-rewired build",
        "test": "react-app-rewired test",
        "prepare": "husky install",
        "format": "prettier --check '**/*.{js,jsx,ts,tsx}'",
        "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "lint": "eslint --ext js,jsx,ts,tsx .",
        "lint:fix": "eslint --fix --ext js,jsx,ts,tsx .",
        "lint-staged": "lint-staged --config .lint-staged.js",
        "typecheck": "tsc",
        "depcheck": "madge -c src",
        "depgraph": "madge --image graph.svg src",
        "generate-flow-types": "cd flow_deps && ./fetch-flow.sh && ./flow-bin/flowctl json-schema | ./flow-bin/flowctl schemalate typescript --name Catalog --hoist-definitions > flow.d.ts",
        "save-local-supabase-data": "cd supabase_data && ./fetch-connector_tags.sh",
        "__build_deploy__": "npm run build; serve -s build",
        "maintenance:on": "touch public/maintenance",
        "maintenance:off": "rm public/maintenance",
        "pre_commit": "npm run lint-staged; npm run depcheck"
    },
    "dependencies": {
        "@emotion/react": "^11.10.0",
        "@emotion/styled": "^11.10.0",
        "@jsonforms/core": "^3.0.0",
        "@jsonforms/material-renderers": "^3.0.0",
        "@jsonforms/react": "^3.0.0",
        "@monaco-editor/react": "^4.4.5",
        "@mui/icons-material": "^5.8.4",
        "@mui/lab": "^5.0.0-alpha.87",
        "@mui/material": "^5.9.3",
        "@mui/utils": "^5.9.3",
        "@mui/x-data-grid": "^5.15.1",
        "@mui/x-date-pickers": "^5.0.8",
        "@supabase/postgrest-js": "^0.37.4",
        "@supabase/supabase-js": "^1.35.5",
        "@supabase/ui": "^0.36.5",
        "ansicolor": "^1.1.100",
        "data-plane-gateway": "https://gitpkg.now.sh/estuary/data-plane-gateway/client/dist?main",
        "date-fns": "^2.29.1",
        "filter-obj": "^3.0.0",
        "immer": "^9.0.15",
        "jose": "^4.8.3",
        "json-ptr": "^3.1.1",
        "lodash": "^4.17.21",
        "logrocket": "^3.0.1",
        "logrocket-react": "^5.0.1",
        "lru-cache": "^7.13.2",
        "madge": "^5.0.1",
        "monaco-editor": "^0.33.0",
        "notistack": "^2.0.5",
        "p-limit": "^4.0.0",
        "path-list-to-tree": "^1.1.1",
        "pretty-bytes": "^6.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.4",
        "react-google-button": "^0.7.2",
        "react-gtm-module": "^2.0.11",
        "react-intl": "^5.25.1",
        "react-json-view": "^1.21.3",
        "react-mui-dropzone": "^4.0.6",
        "react-reflex": "^4.0.9",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0",
        "react-social-login-buttons": "^3.6.1",
        "react-stay-scrolled": "^8.0.0",
        "react-use": "^17.4.0",
        "safe-stable-stringify": "^2.3.1",
        "serve": "^13.0.4",
        "swr": "^1.3.0",
        "use-constant": "^1.1.1",
        "use-local-storage-state": "^17.2.0",
        "zustand": "^4.1.4"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^12.1.5",
        "@testing-library/user-event": "^14.4.3",
        "@types/auth0-js": "^9.14.6",
        "@types/lodash": "^4.14.182",
        "@types/logrocket-react": "^3.0.0",
        "@types/react": "^17.0.48",
        "@types/react-dom": "^17.0.17",
        "@types/react-gtm-module": "^2.0.1",
        "@types/react-lazylog": "^4.5.1",
        "eslint-config-kentcdodds": "^20.3.1",
        "eslint-plugin-jest-dom": "^4.0.2",
        "eslint-plugin-react": "^7.30.1",
        "husky": "^7.0.4",
        "jest-localstorage-mock": "^2.4.21",
        "jest-mock-extended": "^2.0.7",
        "lint-staged": "^12.5.0",
        "node-polyfill-webpack-plugin": "^1.1.4",
        "prettier": "^2.7.1",
        "prettier-plugin-organize-imports": "^3.1.1",
        "react-app-rewired": "^2.2.1",
        "react-scripts": "5.0.0",
        "tsc-files": "^1.1.3",
        "typescript": "^4.7.4"
    },
    "browserslist": {
        "production": [
            ">1%",
            "not dead",
            "Firefox ESR",
            "not ie < 11"
        ],
        "development": [
            "last 2 chrome version",
            "last 2 firefox version",
            "last 2 edge versions",
            "last 2 safari version"
        ]
    },
    "eslintIgnore": [
        "node_modules",
        "build",
        "*.json",
        ".vscode",
        "flow_deps"
    ],
    "jest": {
        "resetMocks": false,
        "verbose": true,
        "testTimeout": 10000
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/estuary/ui.git"
    }
}
