{
    "name": "estuary-ui",
    "version": "0.2.0-alpha",
    "description": "A web based UI to assist in working with Estuary Flow",
    "main": "index.js",
    "private": true,
    "scripts": {
        "start": "set WDS_SOCKET_PORT=0 && react-app-rewired start",
        "build": "react-app-rewired build && compress-cra",
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "test": "react-app-rewired test",
        "prepare": "husky install",
        "format": "prettier --check '**/*.{js,jsx,ts,tsx}'",
        "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "lint": "eslint --ext js,jsx,ts,tsx .",
        "lint:fix": "eslint --fix --ext js,jsx,ts,tsx .",
        "lint-staged": "lint-staged --config .lint-staged.js",
        "typecheck": "tsc",
        "depcheck": "madge -c src",
        "depgraph": "madge --image flow_ui_dep_graph.svg src",
        "generate-flow-types": "cd flow_deps && ./fetch-flow.sh && ./flow-bin/flowctl json-schema | ./flow-bin/flowctl schemalate typescript --name Catalog --hoist-definitions > flow.d.ts",
        "save-local-supabase-data": "cd supabase_data && ./fetch-connector_tags.sh",
        "__build_deploy__": "npm run build; serve -s build",
        "maintenance:on": "touch public/maintenance",
        "maintenance:off": "rm public/maintenance",
        "pre_commit": "npm run lint-staged; npm run depcheck"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.0.8",
        "@dnd-kit/modifiers": "^6.0.1",
        "@dnd-kit/sortable": "^7.0.2",
        "@dnd-kit/utilities": "^3.2.1",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@estuary/flow-web": "^0.2.0",
        "@jsonforms/core": "^3.0.0",
        "@jsonforms/material-renderers": "^3.0.0",
        "@jsonforms/react": "^3.0.0",
        "@monaco-editor/react": "^4.4.6",
        "@mui/icons-material": "^5.11.0",
        "@mui/lab": "^5.0.0-alpha.114",
        "@mui/material": "^5.11.3",
        "@mui/utils": "^5.11.2",
        "@mui/x-data-grid": "^5.17.18",
        "@mui/x-date-pickers": "^5.0.13",
        "@stripe/react-stripe-js": "^2.1.0",
        "@stripe/stripe-js": "^1.52.1",
        "@supabase/postgrest-js": "^0.37.4",
        "@supabase/supabase-js": "^1.35.5",
        "@supabase/ui": "^0.36.5",
        "ansicolor": "^1.1.100",
        "data-plane-gateway": "https://gitpkg.now.sh/estuary/data-plane-gateway/client/dist?main",
        "date-fns": "^2.30.0",
        "date-fns-tz": "^2.0.0",
        "echarts": "^5.4.2",
        "filter-obj": "^5.1.0",
        "iconoir-react": "^6.9.0",
        "immer": "^9.0.21",
        "jose": "^4.14.4",
        "json-ptr": "^3.1.1",
        "lodash": "^4.17.21",
        "logrocket": "^3.0.1",
        "logrocket-react": "^5.0.1",
        "lru-cache": "^9.1.2",
        "madge": "^5.0.2",
        "material-ui-popup-state": "^5.0.9",
        "monaco-editor": "^0.34.1",
        "notistack": "^2.0.8",
        "p-limit": "^4.0.0",
        "path-list-to-tree": "^1.1.1",
        "pretty-bytes": "^6.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^4.0.10",
        "react-google-button": "^0.7.2",
        "react-gtm-module": "^2.0.11",
        "react-intl": "^6.4.4",
        "react-json-view": "^1.21.3",
        "react-mui-dropzone": "^4.0.6",
        "react-reflex": "^4.1.0",
        "react-router": "^6.10.0",
        "react-router-dom": "^6.10.0",
        "react-social-login-buttons": "^3.9.1",
        "react-stay-scrolled": "^8.0.1",
        "react-use": "^17.4.0",
        "react-virtualized-auto-sizer": "^1.0.20",
        "react-window": "^1.8.9",
        "readable-numbers": "^1.0.7",
        "safe-stable-stringify": "^2.4.3",
        "serve": "^14.2.0",
        "stripe": "^12.1.1",
        "swr": "^1.3.0",
        "use-constant": "^1.1.1",
        "use-local-storage-state": "^17.3.0",
        "use-query-params": "^2.2.1",
        "zustand": "^4.3.9"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^12.1.5",
        "@testing-library/user-event": "^14.4.3",
        "@types/auth0-js": "^9.14.7",
        "@types/lodash": "^4.14.191",
        "@types/logrocket-react": "^3.0.0",
        "@types/react": "^17.0.52",
        "@types/react-dom": "^17.0.18",
        "@types/react-gtm-module": "^2.0.1",
        "@types/react-lazylog": "^4.5.1",
        "@types/react-window": "^1.8.5",
        "compress-create-react-app": "^1.4.1",
        "eslint-config-kentcdodds": "^20.3.1",
        "eslint-plugin-jest-dom": "^4.0.3",
        "eslint-plugin-react": "^7.31.11",
        "husky": "^8.0.3",
        "jest-localstorage-mock": "^2.4.26",
        "jest-mock-extended": "^3.0.1",
        "lint-staged": "^13.1.0",
        "node-polyfill-webpack-plugin": "^2.0.1",
        "prettier": "^2.8.1",
        "prettier-plugin-organize-imports": "^3.2.1",
        "react-app-rewired": "^2.2.1",
        "react-scripts": "5.0.1",
        "source-map-explorer": "^2.5.3",
        "tsc-files": "^1.1.3",
        "typescript": "^4.9.0",
        "wasm-loader": "^1.3.0"
    },
    "browserslist": {
        "production": [
            ">1%",
            "not dead",
            "Firefox ESR",
            "not ie < 11"
        ],
        "development": [
            "last 2 chrome version",
            "last 2 firefox version",
            "last 2 edge versions",
            "last 2 safari version"
        ]
    },
    "eslintIgnore": [
        "node_modules",
        "build",
        "*.json",
        ".vscode",
        "flow_deps"
    ],
    "jest": {
        "resetMocks": false,
        "verbose": true,
        "testTimeout": 10000
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/estuary/ui.git"
    }
}
